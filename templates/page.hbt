<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link  rel="stylesheet" href="//fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,700|Source+Code+Pro:400,700" />
  <link  rel="stylesheet" href="{{root}}main.css" />
  <title>{{title}} || Phosphor</title>
</head>
<body class="ui">
  <header>
    <div class="container">
      <a href="/" class="logo">Phosphor</a>
      <nav>
        <ul>
          <li><a href="https://github.com/phosphorjs/">GitHub</a></li>
        </ul>
      </nav>
    </div>
  </header>
  <div class="container main">
    <div class="row">
      <div class="col-3">
        <nav class="navigation">
          <ul>
            {{#each collections.pages}}
                <li {{#ifeq this.title ../../title }} class="selected" {{else}} {{/ifeq}}>
                  <a href="{{root}}{{this.path}}" {{#ifeq this.title ../../title }} class="current" {{else}} {{/ifeq}}>
                    {{this.title}}
                  </a>
                </li>
            {{/each}}
          </ul>
        </nav>
      </div>
      <div class="col-9 typography">
        <h1>{{title}}</h1>
        {{{contents}}}
      </div>
    </div>
  </div>
  <script>
    // (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    //   (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    //     m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    // })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
    // ga('create', 'UA_XXXXXXXX', 'phosphor');
    // ga('send', 'pageview');
  </script>
</body>
</html>
